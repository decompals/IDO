INCLUDES = -I. -I$(INCLUDESRC) -I$(MINCLUDESRC)
        DEPLIBS = MrmClientDepLibs
LOCAL_LIBRARIES = MrmClientLibs

SRCS=	helloint.c

OBJS=	helloint.o

NormalLibraryObjectRule()
MSimpleUilTarget(helloint)
MSimpleUidTarget(french/uid/localstrings)
MSimpleUidTarget(english/uid/localstrings)
MSimpleUidTarget(hebrew/uid/localstrings)
MSimpleUidTarget(japan/uid/localstrings)

install::
	$(RM) $(UIDDIR)/localstrings.uid

InstallNonExecFile(french/uid/localstrings.uid,$(UIDDIR)/french/uid)
InstallNonExecFile(english/uid/localstrings.uid,$(UIDDIR)/english/uid)
InstallNonExecFile(japan/uid/localstrings.uid,$(UIDDIR)/japan/uid)
InstallNonExecFile(hebrew/uid/localstrings.uid,$(UIDDIR)/hebrew/uid)

DependTarget()
#ifdef SGIArchitecture
FILES = Imakefile *.c *.uil README *\/*.bdf */uid\/*.uil
InstallMultiple($(FILES),$(MDEMOSRCDIR))
#endif /* sgi */
