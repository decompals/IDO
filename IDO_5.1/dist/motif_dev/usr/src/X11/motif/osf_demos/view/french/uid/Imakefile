INCLUDES = -I. -I$(MINCLUDESRC) -I$(UILSRC) -I$(MTOOLKITSRC) -I$(LIBSRC)

.uil.uid:
	$(RM) $@
	$(UIL) -o $@ $*.uil  -I$(UILSRC)

UILS1= fileview.uil

NormalLibraryObjectRule()

MComplexUidTarget_1(fileview)

#ifdef SGIArchitecture
FILES = Imakefile *.uil
InstallMultiple($(FILES),$(MDEMOSRCDIR))
#else /* OSF original */
install::
	$(RM) $(UIDDIR)/fileview.uid

InstallNonExecFile(fileview.uid,$(UIDDIR)/french/uid)
#endif /* sgi */
