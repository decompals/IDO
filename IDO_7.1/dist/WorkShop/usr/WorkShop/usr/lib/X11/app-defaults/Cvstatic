! valid range {1-128}
Cvstatic*indentationWidth:		15

Cvstatic*nameAlign:			true
Cvstatic*arglistAlign:			true

Cvstatic*memberOrder for C++ Class:     Protection: PUBLIC [public], PROTECTED* [protected], PRIVATE* [private]; Scope: INSTANCE [instance], STATIC [static]; Member: TYPES* [type], DATA [data], METHODS [method], VIRTUAL METHODS [virtualmethod];

Cvstatic*relationOrder for C++ Class:   BASE CLASSES [base], DERIVED CLASSES [derived], USES [uses], USED BY [usedby], FRIEND FUNCTIONS [friendfunction], FRIENDS [friend], FRIEND OF [friendof]

Cvstatic*memberOrder for C++ Template Definition:     Protection: PUBLIC [public], PROTECTED* [protected], PRIVATE* [private]; Scope: INSTANCE [instance], STATIC [static]; Member: TYPES* [type], DATA [data], METHODS [method], VIRTUAL METHODS [virtualmethod];

Cvstatic*relationOrder for C++ Template Definition:   BASE CLASSES [base], DERIVED CLASSES [derived], USES [uses], USED BY [usedby], FRIEND FUNCTIONS [friendfunction], FRIENDS [friend], FRIEND OF [friendof]

Cvstatic*memberOrder for C++ Template Instance:     Protection: PUBLIC [public], PROTECTED* [protected], PRIVATE* [private]; Scope: INSTANCE [instance], STATIC [static]; Member: TYPES* [type], DATA [data], METHODS [method], VIRTUAL METHODS [virtualmethod];

Cvstatic*relationOrder for C++ Template Instance:   BASE CLASSES [base], DERIVED CLASSES [derived], USES [uses], USED BY [usedby], FRIEND FUNCTIONS [friendfunction], FRIENDS [friend], FRIEND OF [friendof]

Cvstatic*memberOrder for Ada Package:     Protection: SPEC PUBLIC [public], SPEC PRIVATE* [protected], BODY* [private]; Scope: CONTAINS [instance], DECLARES [static]; Member: TYPES* [type], DATA [data], FUNCTIONS [method], PRIMITIVE OPERATIONS [virtualmethod];

Cvstatic*relationOrder for Ada Package:   PARENTS [base], DERIVED [derived], USES [uses], USED BY [usedby], FRIEND FUNCTIONS [friendfunction], FRIENDS [friend], FRIEND OF [friendof]

