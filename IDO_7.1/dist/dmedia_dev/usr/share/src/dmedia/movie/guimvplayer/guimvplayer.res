
!
! X Resources for "guimvplayer", the demo program showing how to 
! add a movie easily to an IRIS IM program using the MovieController class
! 

guimvplayer*keyboardFocusPolicy: pointer
guimvplayer*background:          #9eb2b5

guimvplayer*XmLabel*background:  #c593c5

guimvplayer*movie*popup_popup*background:grey
guimvplayer*movie*popup_popup*fontList:-adobe-times-medium-r-*--12-*-*-*-*-*-*-1
guimvplayer*movie*popup_popup*marginTop:0
guimvplayer*movie*popup_popup*marginBottom:0
guimvplayer*movie*popup_popup*marginLeft:0
guimvplayer*movie*popup_popup*marginRight:0
guimvplayer*movie*popup_popup*marginWidth:0
guimvplayer*movie*popup_popup*marginHeight:0
