        DEPLIBS = XmClientDepLibs
LOCAL_LIBRARIES = XmClientLibs

SRCS = motifshell.c
OBJS = motifshell.o

MSimpleProgramTarget(motifshell)
InstallProgram(motifshell,$(BINDIR))

#ifdef SGIArchitecture
InstallNonExec(motif, $(MDEMOSRCDIR))
InstallNonExec(welcome, $(MDEMOSRCDIR))
InstallNonExec(research, $(MDEMOSRCDIR))
InstallNonExec(principles, $(MDEMOSRCDIR))
InstallNonExec(membership, $(MDEMOSRCDIR))
InstallNonExec(help, $(MDEMOSRCDIR))
#else /* OSF original */
InstallNonExec(motif,$(BINDIR))
InstallNonExec(welcome,$(BINDIR))
InstallNonExec(research,$(BINDIR))
InstallNonExec(principles,$(BINDIR))
InstallNonExec(membership,$(BINDIR))
InstallNonExec(help,$(BINDIR))
#endif /* SGIArchitecture */

DependTarget()
#ifdef SGIArchitecture
FILES = Imakefile README *.c *.h
InstallMultiple($(FILES),$(MDEMOSRCDIR))
#endif /* SGIArchitecture */
