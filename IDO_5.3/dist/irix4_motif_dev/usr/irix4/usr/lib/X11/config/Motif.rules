#ifdef SGIArchitecture
/*
 * NOTE: this file is exactly the OSF one, except for the following two things:
 *
 *	* It has been R4-ized in its link rules
 *
 *	* It includes Motif.rules.sgi -- the reason for this aberrant behavior
 *	  is because I don't want to mess with the X group's 'sgi.cf' file.
 */
#endif
#ifndef MSimpleProgramTarget
#define MSimpleProgramTarget(program)                                   @@\
	                                                                @@\
install all:: program                                                           @@\
	                                                                @@\
program: program.o $(DEPS) $(LOCAL_LIBRARIES)                           @@\
	RemoveTargetProgram($@)						@@\
	$(CC) -o $@ program.o $(LDOPTIONS) $(DEPS) $(LOCAL_LIBRARIES) $(LDLIBS) $(SYSLIBS) $(EXTRA_LOAD_FLAGS) @@\
	                                                                @@\
clean::                                                                 @@\
	$(RM) program                                                   @@\
	$(RM) program.o
#endif

#ifndef MSimpleUilTarget
#define MSimpleUilTarget(program)                                   @@\
	                                                                @@\
install all:: program                                                           @@\
	                                                                @@\
program: program.o program.uid $(DEPS) $(LOCAL_LIBRARIES)               @@\
	RemoveTargetProgram($@)						@@\
	$(CC) -o $@ program.o $(LDOPTIONS) $(DEPS) $(LOCAL_LIBRARIES) $(LDLIBS) $(SYSLIBS) $(EXTRA_LOAD_FLAGS) @@\
	                                                                @@\
program.uid: program.uil                                                @@\
	$(RM) $@                                                        @@\
	$(UIL) -o $@ program.uil  -I$(UILSRC)                           @@\
	                                                                @@\
InstallProgram(program,$(BINDIR))					@@\
	                                                                @@\
	                                                                @@\
clean::                                                                 @@\
	$(RM) program                                                   @@\
	$(RM) program.uid                                               @@\
	$(RM) program.o 
#endif

