       DEPXMLIB = $(ROOT)$(SHLIBDIR)/libXm.so
          XMLIB = -L$(ROOT)$(SHLIBDIR) -lXm
       DEPMRMLIB = $(ROOT)$(SHLIBDIR)/libMrm.so
          MRMLIB = -L$(ROOT)$(SHLIBDIR) -lMrm
#endif /* SGIArchitecture */

        DEPLIBS = MrmClientDepLibs
LOCAL_LIBRARIES = MrmClientLibs

SRCS = textedit.c tk.c dlg.c app.c file.c
OBJS = textedit.o tk.o dlg.o app.o file.o
UILS = textedit.uil strings.uil /* For documentation only */

MComplexProgramTarget(textedit,$(LOCAL_LIBRARIES),NullParameter)
MSimpleUidTarget(textedit)

#ifdef SGIArchitecture
FILES = Imakefile *.c *.h *.uil
InstallMultiple($(FILES),$(MDEMOSRCDIR))
#endif /* SGIArchitecture */
