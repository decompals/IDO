#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

SUBDIRS = motifshell periodic clipboard \
	motifanim motifburger hellomotif \
	helloint dogs xmtravel xmpiano \
	xmsamplers motifgif uilsymdump view textedit DragAndDrop

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
MakeLintLibSubdirs($(SUBDIRS))
#ifdef SGIArchitecture
FILES = Imakefile
InstallMultiple($(FILES),$(MDEMOSRCDIR))
#endif /* sgi */
