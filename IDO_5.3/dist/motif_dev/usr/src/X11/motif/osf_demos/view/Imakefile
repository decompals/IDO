#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

SUBDIRS = uid french german american

MakeSubdirs($(SUBDIRS))

#ifdef SGIArchitecture /* Quickstart Fix for SGI */
       DEPXMLIB = $(ROOT)$(SHLIBDIR)/libXm.so
          XMLIB = -L$(ROOT)$(SHLIBDIR) -lXm
       DEPMRMLIB = $(ROOT)$(SHLIBDIR)/libMrm.so
          MRMLIB = -L$(ROOT)$(SHLIBDIR) -lMrm
#endif /* SGIArchitecture */

        DEPLIBS = MrmClientDepLibs
LOCAL_LIBRARIES = MrmClientLibs

SRCS = main.c text.c file.c
OBJS = main.o text.o file.o

MComplexProgramTarget(fileview,$(LOCAL_LIBRARIES),NullParameter)

InstallAppDefaults(Fileview)
#ifdef SGIArchitecture
FILES = Imakefile README xmfile *.c *.h
InstallMultiple($(FILES),$(MDEMOSRCDIR))
#endif /* SGIArchitecture */
